{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
      "useBuiltIns": "usage",
      "corejs": "3.6.5"
       }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-react-jsx",
    [
      "@babel/plugin-transform-class-properties",
      { "loose": true }
    ],
    [
      "@babel/plugin-transform-private-methods",
      { "loose": true }
    ],
    [
      "@babel/plugin-transform-private-property-in-object",
      { "loose": true }
    ]
  ]
}
